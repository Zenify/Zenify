{
	"name": "zenify/doctrine-fixtures",
	"description": "Doctrine Fixtures, Faker and Alice integration into Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": "^7.0",
		"fzaninotto/faker": "~1.5",
		"nelmio/alice": "~2.0",
		"kdyby/doctrine": "~3.0",

		"nette/di": "~2.4",
		"nette/finder": "~2.4",
		"nette/reflection": "~2.4",
		"symfony/console": "^3.0",
		"tracy/tracy": "~2.4"
	},
	"require-dev": {
		"zenify/coding-standard": "^4.0",
		"nette/bootstrap": "~2.4",
		"phpunit/phpunit": "^5.6"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineFixtures\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineFixtures\\Tests\\": "tests"
		}
	},
	"scripts": {
		"check-cs": "vendor/bin/phpcs src tests -ps --standard=vendor/zenify/coding-standard/src/ZenifyCodingStandard/ruleset.xml"
	}
}
